{
  "version": 2, 
  "builds": [
     { "src": "*.js", "use": "@now/node" },
     { "src": "public/**", "use": "@now/static" }
  ],
  "routes": [
    { "src": "/", "dest": "/public/host.html" },
    { "src": "\/l\/(?:[A-Za-z0-9+/]{4})*(?:[A-Za-z0-9+/]{2}==|[A-Za-z0-9+/]{3}=)", "dest": "/public/guest.html"}
  ]
}